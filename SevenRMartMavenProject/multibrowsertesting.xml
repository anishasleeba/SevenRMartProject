<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="multibrowsertesting" parallel="tests">
	<test thread-count="5" name="ChromeTest">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testscripts.AdminUsersTest" />
			<class name="testscripts.ManageNewstest" />
			<class name="testscripts.ManageCategoryTest" />
			<class name="testscripts.ManageContactTest" />
			<class name="testscripts.ManageFooterTest" />
			<class name="testscripts.LoginTest" />
			<class name="testscripts.LogoutTest" />
		</classes>
	</test> <!--Test -->
	<test thread-count="5" name="FirefoxTest">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="testscripts.AdminUsersTest" />
			<class name="testscripts.ManageNewstest" />
			<class name="testscripts.ManageCategoryTest" />
			<class name="testscripts.ManageContactTest" />
			<class name="testscripts.ManageFooterTest" />
			<class name="testscripts.LoginTest" />
			<class name="testscripts.LogoutTest" />
		</classes>
	</test> <!--Test -->
</suite> <!--Suite -->
